{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 450,
  "height": 300,
  "data": {"url": "https://raw.githubusercontent.com/ZaneeeY/Population_visualization/main/data/world_population_new.csv"},
  "mark": "bar",
  "encoding": {
    "x": {"aggregate": "sum", "field": "Population"},
